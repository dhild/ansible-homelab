# ansible managed

configDirectory=/etc/pihole
installDirectory=/opt/pihole
PIHOLE_INTERFACE={{ ansible_default_ipv4.interface }}
IPV4_ADDRESS={{ ansible_default_ipv4.network }}/24
IPV6_ADDRESS=""
QUERY_LOGGING=true
INSTALL_WEB=true
DNSMASQ_LISTENING=single
PIHOLE_DNS_1={{ dns_servers[0] }}
PIHOLE_DNS_2={{ dns_servers[1] }}
DNS_FQDN_REQUIRED=true
DNS_BOGUS_PRIV=true
DNSSEC=true
CONDITIONAL_FORWARDING=false
